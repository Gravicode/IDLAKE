<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    <!-- Include meta tag to ensure proper rendering and touch zooming -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/styles/css/themes/default/jquery.mobile-1.4.5.min.css">
    <link rel="shortcut icon" href="/styles/favicon.ico">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,700">
    <link href="~/styles/css/themes/default/EditStyle.css" rel="stylesheet" />
    <script src="/styles/js/jquery.js"></script>
    <script src="/styles/js/jquery.mobile-1.4.5.min.js"></script>

    <!--Reference the SignalR library. -->

    <script src="/scripts/jquery.signalR-2.2.0.min.js"></script>

    <!--Reference the autogenerated SignalR hub script. -->

    <script src="/signalr/hubs"></script>

    <!--Add script to update the page and send messages.-->
    <meta charset="utf-8" />
</head>
<body>
    @if (!System.Web.HttpContext.Current.User.Identity.IsAuthenticated) {
        string url = Request.Url.Authority + HttpContext.Current.Request.RawUrl.ToString();

        if (Request.ServerVariables["HTTPS"] == "on")
        {
            url = "https://" + url;
        }
        else
        {
            url = "http://" + url;
        }
        Response.Redirect("/pages/publik/login/?ReturnUrl="+url); }
    @RenderBody()
</body>
</html>
